lcmd - j : aerospace focus --boundaries workspace --boundaries-action wrap-around-the-workspace down
lcmd - k : aerospace focus --boundaries workspace --boundaries-action wrap-around-the-workspace up
lcmd - h : aerospace focus --boundaries workspace --boundaries-action wrap-around-the-workspace left
lcmd - l : aerospace focus --boundaries workspace --boundaries-action wrap-around-the-workspace right
ralt + shift - return : /Applications/kitty.app/Contents/MacOS/kitty --single-instance -d ~
ralt - return : open -n /Applications/Alacritty.app
ralt - t : open -n /System/Applications/Utilities/Terminal.app
ralt - d : /Applications/Google\ Chrome.app/Contents/MacOS/Google\ Chrome
lcmd - t : aerospace layout floating tiling
# lcmd - b : yabai -m window --toggle border 

# shift + lcmd - j : yabai -m window --swap next
# shift + lcmd - k : yabai -m window --swap prev
shift + lcmd - h : aerospace join-with left
shift + lcmd - l : aerospace join-with right


ralt - space : aerospace fullscreen

lcmd - a : aerospace workspace $(aerospace list-workspaces --monitor focused | sed -n 1p)
lcmd - s : aerospace workspace $(aerospace list-workspaces --monitor focused | sed -n 2p)
lcmd - d : aerospace workspace $(aerospace list-workspaces --monitor focused | sed -n 3p)
lcmd - f : aerospace workspace $(aerospace list-workspaces --monitor focused | sed -n 4p)
# lcmd - u : yabai -m space --focus $(yabai -m query --spaces --display | jq '.[4]["index"]')
# lcmd - i : yabai -m space --focus $(yabai -m query --spaces --display | jq '.[5]["index"]')
# lcmd - o : yabai -m space --focus $(yabai -m query --spaces --display | jq '.[6]["index"]')
# lcmd - p : yabai -m space --focus $(yabai -m query --spaces --display | jq '.[7]["index"]')


lcmd + shift - a : aerospace move-node-to-workspace $(aerospace list-workspaces --monitor focused | sed -n 1p)
lcmd + shift - s : aerospace move-node-to-workspace $(aerospace list-workspaces --monitor focused | sed -n 2p)
lcmd + shift - d : aerospace move-node-to-workspace $(aerospace list-workspaces --monitor focused | sed -n 3p)
lcmd + shift - f : aerospace move-node-to-workspace $(aerospace list-workspaces --monitor focused | sed -n 4p)
# lcmd + shift - u : yabai -m window --space $(yabai -m query --spaces --display | jq '.[4]["index"]')
# lcmd + shift - i : yabai -m window --space $(yabai -m query --spaces --display | jq '.[5]["index"]')
# lcmd + shift - o : yabai -m window --space $(yabai -m query --spaces --display | jq '.[6]["index"]')
# lcmd + shift - p : yabai -m window --space $(yabai -m query --spaces --display | jq '.[7]["index"]')

lcmd - space : yabai -m window --toggle split

lcmd - w : aerospace focus-monitor next
lcmd - q : aerospace focus-monitor prev
lcmd + shift -w : aerospace move-node-to-monitor next
lcmd + shift -q : aerospace move-node-to-monitor prev

lcmd + shift - r : ~/.cargo/bin/alacritty --command zsh -c "/usr/local/bin/code --folder-uri \"\$(node -e 'console.log(decodeURIComponent(process.argv[1]))' -- \"\$(cat ~/Library/Application\ Support/Code/storage.json | jq '.openedPathsList.entries[].folderUri' -r | ~/.fzf/bin/fzf  )\")\""

